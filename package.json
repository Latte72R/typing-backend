{
  "name": "typing-backend",
  "version": "0.1.0",
  "description": "Core domain logic for the typing backend service.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "TS_NODE_TRANSPILE_ONLY=1 node --loader ts-node/esm src/server/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "node --loader ts-node/esm --test tests/**/*.test.ts",
    "lint": "biome lint src tests prisma scripts package.json biome.json",
    "format": "biome format .",
    "start": "node dist/server/main.js",
    "seed:prompts": "ts-node --esm scripts/bulkImportPrompts.ts"
  },
  "keywords": [
    "typing",
    "backend",
    "contest",
    "leaderboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.0",
    "fastify-type-provider-zod": "^5.1.0",
    "pg": "^8.11.3",
    "socket.io": "^4.8.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.30",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
